<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.kh.team.member">
	<select id="login" resultType="MemberVo">
		select *
		from member
		where m_id = #{m_id}
		and m_pass = #{m_pass}
	</select>
</mapper>