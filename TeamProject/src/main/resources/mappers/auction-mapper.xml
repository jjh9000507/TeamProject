<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
<mapper namespace="com.kh.team.auction">

	<select id="getAuctionList" resultType="AuctionVo">
		select i.img, a.*
		from auction_img i, auction a where i.p_no = a.p_no and i.main_img = '1'
	</select>

	<select id="getAuctionImgMain" resultType="AuctionImgVo">
		select * from auction_img
	</select>
</mapper>